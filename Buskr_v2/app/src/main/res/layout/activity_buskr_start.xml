<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context=".Buskr_start">

    <ImageView android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="90dp"
        android:adjustViewBounds="true"
        android:maxWidth="300dp"
        android:maxHeight="130dp"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:src="@drawable/buskrfronttext"
        android:id="@+id/textImage" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bypass"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:id="@+id/button4"
        android:onClick="bypass"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/settingsButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="20dp"
        android:id="@+id/button1"
        android:onClick="enterSettings"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/musicButton"
        android:gravity="center"
        android:id="@+id/button2"
        android:onClick="enterMusic"
        android:layout_alignTop="@+id/button1"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mapButton"
        android:id="@+id/button3"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="20dp"
        android:onClick="enterMap"/>

    <ImageView
        android:id="@+id/frontpageAnim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp" />

</RelativeLayout>
